<div class="row">
    <div class="col-8 offset-2">
        <div class="form-group">
            <select id="optionFile" class="form-control">
                <option value="1">Json</option>
                <option value="2">Pipes</option>
            </select>
        </div>

        <div class="form-group">
            <button class="btn btn-success" onclick="generate()">Generar</button>
        </div>
    </div>
</div>

<script>
    async function generate() {
        // code
        let optionFile = document.getElementById('optionFile').value;

        await fetch('@Url.Content("~/GeneratorFile/CreateFile")' + "?optionFile=" + optionFile)
            .then(res => res.json())
            .then(json => alert(json));
    }
</script>